    <version>0.8.0-SNAPSHOT</version>
  </parent>
  <!--Used to build the symbolic name of the bundle-->
  <artifactId>ccl.northwestern.edu.netlogo5</artifactId>
  <version>5.0.3</version>
  <!-- ************************************************************* -->
  <!-- *** Build Settings ****************************************** -->
  <!-- ************************************************************* -->
  <packaging>bundle</packaging>
